{
	"name": "@e280/stz",
	"version": "0.2.21",
	"description": "everyday ts fns for everything",
	"license": "MIT",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"type": "module",
	"main": "./x/index.js",
	"sideEffects": false,
	"exports": {
		".": "./x/index.js"
	},
	"files": [
		"x",
		"s"
	],
	"scripts": {
		"build": "rm -rf x && mkdir x && ln -s '$(realpath node_modules)' x/node_modules && tsc",
		"dev": "tsc -w & node --watch x/tests.test.js & wait",
		"test": "node x/tests.test.js",
		"count": "find s -path '*/_archive' -prune -o -name '*.ts' -exec wc -l {} +"
	},
	"devDependencies": {
		"@e280/science": "^0.1.8",
		"@types/node": "^25.3.0",
		"typescript": "^5.9.3"
	},
	"keywords": [
		"standard",
		"std",
		"steeze"
	],
	"homepage": "https://github.com/e280/stz#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/e280/stz.git"
	},
	"bugs": {
		"url": "https://github.com/e280/stz/issues"
	}
}
