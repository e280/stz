{
	"name": "@e280/stz",
	"version": "0.0.0-11",
	"description": "everyday ts fns for everything",
	"license": "MIT",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"type": "module",
	"main": "x/index.js",
	"files": [
		"x",
		"s"
	],
	"scripts": {
		"build": "run-s _clean _links _tsc",
		"test": "node x/tests.test.js",
		"count": "find s -path '*/_archive' -prune -o -name '*.ts' -exec wc -l {} +",
		"start": "run-p _tscw _testw",
		"_clean": "rm -rf x && mkdir x",
		"_links": "ln -s \"$(realpath node_modules)\" x/node_modules",
		"_tsc": "tsc",
		"_tscw": "tsc -w",
		"_testw": "node --watch x/tests.test.js"
	},
	"devDependencies": {
		"@e280/science": "^0.0.4",
		"@types/node": "^22.15.15",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.8.3"
	},
	"keywords": [
		"standard",
		"std",
		"steeze"
	],
	"homepage": "https://github.com/e280/stz#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/e280/stz.git"
	},
	"bugs": {
		"url": "https://github.com/e280/stz/issues"
	}
}
